{
  "name": "hash-branch-name",
  "version": "1.0.0",
  "private": true,
  "description": "A small github action to convert your branch name into a unique SHA. Use this for example if you plan to do per branch releases.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "keywords": [
    "actions",
    "node",
    "hash",
    "branch",
    "ci"
  ],
  "author": "felix-schaipp",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/parser": "^5.26.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.4.2",
    "jest": "^28.1.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.8.2"
  },
  "engines": {
    "npm": "please use yarn",
    "yarn": ">1.22.0"
  }
}
